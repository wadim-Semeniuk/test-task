{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "types": ["@nuxt/types"],
    "paths": {
      "@/*": ["./*"],
    }
  },
  "exclude": ["node_modules", ".nuxt", "dist"]
}
